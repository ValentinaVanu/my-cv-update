-
  var header = '#Skills'
  var whatICanDoList = [
    {
      title: 'Front End',
      skillList: [
        'HTML5',
        'CSS3',
        'JavaScript',
        'Bootstrap',
        'Jade',
        'Pug',
        'JQuery',
        'Gulp',
        'Yeoman',
        'Sass',
      ]
    },
    {
      title: 'Back End',
      skillList: [
        'Json',
        'Git',
        'NodeJs',
      ]
    },
    {
      title: 'Others',
      skillList: [
        'Adobe Lightroom',
        'Adobe Illustrator',
        'Microsoft Word',
      ]
    }
  ];
#what-i-can-do
.what-i-can-do
  .what-i-can-do__header=header
  each whatICanDo in whatICanDoList
    .what-i-can-do__title=whatICanDo.title
    .what-i-can-do__list
      each skill in whatICanDo.skillList
        .what-i-can-do__skill=skill